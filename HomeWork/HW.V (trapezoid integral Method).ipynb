   "source": [
    "# trapezoid integral Method\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "a = 0.2\n",
    "b = 5\n",
    "n = 10\n",
    "h = (b - a) / (n - 1)\n",
    "x = np.linspace(a, b, n)\n",
    "f = (x-1)**2/np.exp(x)\n",
    "\n",
    "I_trap = (h/2)*(f[0] + 2 * sum(f[1:n-1]) + f[n-1])\n",
    "err_trap = 0.676 - I_trap\n",
    "\n",
    "print(I_trap)\n",
    "print(err_trap)"
   ]
  },
